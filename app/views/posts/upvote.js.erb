$('.like_post').bind('ajax:success', function(){
   $(this).closest('.like_post').hide();
   $(this).closest('.votes').html('<%= link_to dislike_post_path(@post), method: :get, remote: true, class: "dislike_post" do %><i class="material-icons love">favorite</i><% end %><span data-toggle="tooltip" data-placement="top" title="" class="time user-name" data-original-title= " <%= @post.liked_users %>">(<%= @post.get_upvotes.size %>)</span>');
   $('[data-toggle="tooltip"]').tooltip();
});
